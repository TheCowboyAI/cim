@prefix cim: <http://cim.thecowboyai.com/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Domain Classes
cim:Domain rdf:type owl:Class .
cim:Category rdf:type owl:Class .
cim:Term rdf:type owl:Class .

# Information Domain
cim:InformationDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Information Domain" ;
    rdfs:comment "Core information representation and processing" .

cim:InformationEntity rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:InformationDomain .

cim:InformationState rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:InformationDomain .

# Knowledge Domain
cim:KnowledgeDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Knowledge Domain" ;
    rdfs:comment "Knowledge representation and management" .

cim:Fact rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:KnowledgeDomain .

cim:Claim rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:KnowledgeDomain .

cim:Theory rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:KnowledgeDomain .

# Organization Domain
cim:OrganizationDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Organization Domain" ;
    rdfs:comment "Organizational structure and management" .

cim:Operator rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:OrganizationDomain .

cim:Account rdf:type owl:Class ;
    rdfs:subClassOf cim:Term ;
    cim:belongsToDomain cim:OrganizationDomain .

# Relationship Properties
cim:contains rdf:type owl:ObjectProperty .
cim:manages rdf:type owl:ObjectProperty .
cim:validates rdf:type owl:ObjectProperty .
cim:dependsOn rdf:type owl:ObjectProperty .
cim:extends rdf:type owl:ObjectProperty .
cim:configures rdf:type owl:ObjectProperty .

# Domain Relationships
cim:Fact cim:validates cim:Claim .
cim:Claim cim:supports cim:Theory .
cim:Theory cim:contains cim:Claim .

cim:Operator cim:manages cim:Account .
cim:Account cim:contains cim:User .
cim:User cim:has cim:Permission .

# Cross-Domain Relationships
cim:InformationEntity cim:dependsOn cim:InformationState .
cim:Agent cim:processes cim:InformationEntity .
cim:Policy cim:governs cim:Behavior .

# Property Characteristics
cim:contains rdfs:domain cim:Term ;
    rdfs:range cim:Term .

cim:manages rdfs:domain cim:Term ;
    rdfs:range cim:Term .

cim:validates rdfs:domain cim:Term ;
    rdfs:range cim:Term .

# Annotations
cim:InformationDomain rdfs:comment "Core information representation and processing"@en .
cim:KnowledgeDomain rdfs:comment "Knowledge representation and management"@en .
cim:OrganizationDomain rdfs:comment "Organizational structure and management"@en .

# New Knowledge Domain concepts
cim:Research rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Research" ;
    rdfs:comment "Systematic investigation process" .

cim:Evidence rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Evidence" ;
    rdfs:comment "Observable data supporting claims" .

cim:Method rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Method" ;
    rdfs:comment "Systematic procedure for research" .

cim:Finding rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Finding" ;
    rdfs:comment "Specific result from research" .

cim:Citation rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Citation" ;
    rdfs:comment "Reference to information source" .

cim:KnowledgeGraph rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Knowledge Graph" ;
    rdfs:comment "Structured knowledge representation" .

cim:Validation rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Validation" ;
    rdfs:comment "Process of verifying knowledge claims" .

cim:Classification rdf:type owl:Class ;
    rdfs:subClassOf cim:KnowledgeDomain ;
    rdfs:label "Classification" ;
    rdfs:comment "System for organizing knowledge entities" .

# New Knowledge Domain relationships
cim:Evidence cim:validates cim:Claim .
cim:Evidence cim:supports cim:Finding .
cim:Evidence cim:contradicts cim:Claim .
cim:Research cim:cites cim:Citation .
cim:Finding cim:derivesFrom cim:Evidence .
cim:Classification cim:classifies cim:KnowledgeDomain .
cim:Claim cim:supports cim:Theory .
cim:Theory cim:contains cim:Claim .
cim:Operator cim:manages cim:Account .
cim:Account cim:contains cim:User .
cim:User cim:has cim:Permission .
cim:InformationEntity cim:dependsOn cim:InformationState .
cim:Agent cim:processes cim:InformationEntity .
cim:Policy cim:governs cim:Behavior .

# Security Domain
cim:SecurityDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Security Domain" ;
    rdfs:comment "Security and access control" .

# Distribution Domain
cim:DistributionDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Distribution Domain" ;
    rdfs:comment "Distributed system components" .

# AI Integration Domain
cim:AIIntegrationDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "AI Integration Domain" ;
    rdfs:comment "AI tools and capabilities" .

# Persistence Domain
cim:PersistenceDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Persistence Domain" ;
    rdfs:comment "Data storage and retrieval" .

# Agent Domain
cim:AgentDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Agent Domain" ;
    rdfs:comment "Autonomous agents and behaviors" .

# Business Domain
cim:BusinessDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Business Domain" ;
    rdfs:comment "Business concepts and value delivery" .

# Environment Domain
cim:EnvironmentDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Environment Domain" ;
    rdfs:comment "Physical and virtual resource management" .

# Governance Domain
cim:GovernanceDomain rdf:type owl:Class ;
    rdfs:subClassOf cim:Domain ;
    rdfs:label "Governance Domain" ;
    rdfs:comment "Rules, policies, and ethical guidelines" .

# Object Properties
cim:hasEvidence rdf:type owl:ObjectProperty ;
    rdfs:domain cim:Research ;
    rdfs:range cim:Evidence ;
    rdfs:label "has evidence" .

cim:usesMethod rdf:type owl:ObjectProperty ;
    rdfs:domain cim:Research ;
    rdfs:range cim:Method ;
    rdfs:label "uses method" .

cim:producesFinding rdf:type owl:ObjectProperty ;
    rdfs:domain cim:Research ;
    rdfs:range cim:Finding ;
    rdfs:label "produces finding" .

cim:hasCitation rdf:type owl:ObjectProperty ;
    rdfs:domain cim:Research ;
    rdfs:range cim:Citation ;
    rdfs:label "has citation" .

cim:representsKnowledge rdf:type owl:ObjectProperty ;
    rdfs:domain cim:KnowledgeGraph ;
    rdfs:range cim:Research ;
    rdfs:label "represents knowledge" .

# Data Properties
cim:hasName rdf:type owl:DatatypeProperty ;
    rdfs:domain cim:Domain ;
    rdfs:range xsd:string ;
    rdfs:label "has name" .

cim:hasDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain cim:Domain ;
    rdfs:range xsd:string ;
    rdfs:label "has description" .

cim:hasTimestamp rdf:type owl:DatatypeProperty ;
    rdfs:domain cim:Research ;
    rdfs:range xsd:dateTime ;
    rdfs:label "has timestamp" .

cim:hasConfidenceLevel rdf:type owl:DatatypeProperty ;
    rdfs:domain cim:Evidence ;
    rdfs:range xsd:decimal ;
    rdfs:label "has confidence level" . 